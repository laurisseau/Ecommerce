     <Button type=\"submit\">Update</Button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\reso0\\OneDrive\\Desktop\\ecommerce\\frontend\\src\\components\\SearchBox.js",[],[],"C:\\Users\\reso0\\OneDrive\\Desktop\\ecommerce\\frontend\\src\\screens\\SearchScreen.js",[],[],"C:\\Users\\reso0\\OneDrive\\Desktop\\ecommerce\\frontend\\src\\components\\ProtectedRoute.js",[],[],{"ruleId":"137","severity":1,"message":"138","line":30,"column":10,"nodeType":"139","messageId":"140","endLine":30,"endColumn":25},{"ruleId":"137","severity":1,"message":"141","line":32,"column":12,"nodeType":"139","messageId":"140","endLine":32,"endColumn":25},"no-unused-vars","'confirmPassword' is assigned a value but never used.","Identifier","unusedVar","'loadingUpdate' is assigned a value but never used."]